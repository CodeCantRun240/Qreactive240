<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile</title>
</head>
<body>
    <h1>Profile</h1>
    <ul>
        <% for (let i = 0; i < properties.length; i++) { %>
            <li><strong><%= properties[i].key %>:</strong> <%= properties[i].value %></li>
        <% } %>
    </ul>
</body>
</html>
